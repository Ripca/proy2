<?xml version="1.0" encoding="UTF-8"?>
<jasperReport xmlns="http://jasperreports.sourceforge.net/jasperreports"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://jasperreports.sourceforge.net/jasperreports
                                  http://jasperreports.sourceforge.net/xsd/jasperreport.xsd"
              name="miReporte" pageWidth="595" pageHeight="842">
    
    <queryString>
        <![CDATA[SELECT 'Test' as mensaje]]>
    </queryString>
    
    <field name="mensaje" class="java.lang.String"/>
    
    <title>
        <band height="50">
            <staticText>
                <reportElement x="0" y="0" width="555" height="30"/>
                <textElement textAlignment="Center">
                    <font size="18" isBold="true"/>
                </textElement>
                <text><![CDATA[Reporte de Prueba]]></text>
            </staticText>
        </band>
    </title>
    
    <detail>
        <band height="25">
            <textField>
                <reportElement x="0" y="0" width="555" height="20"/>
                <textFieldExpression><![CDATA[$F{mensaje}]]></textFieldExpression>
            </textField>
        </band>
    </detail>
    
</jasperReport>